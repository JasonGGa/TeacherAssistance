<Window x:Class="EpieHorarios.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EpieHorarios"
        mc:Ignorable="d"
        Title="Administración" Height="250" Width="300"
        Closed="Window_Closed">
    <Grid>
        <Button x:Name="AdminProfe" Content="Administrar Profesores" HorizontalAlignment="Left" Margin="78,25,0,0" VerticalAlignment="Top" Width="140" Click="AdminProfe_Click"/>
        <Button x:Name="AdminCurso" Content="Administrar Cursos" HorizontalAlignment="Left" Margin="78,70,0,0" VerticalAlignment="Top" Width="140" Click="AdminCurso_Click"/>
        <Button x:Name="AdminAsis" Content="Ver Asistencias" HorizontalAlignment="Left" Margin="78,115,0,0" VerticalAlignment="Top" Width="140" Click="AdminAsis_Click"/>
        <Button x:Name="CambiarContra" Content="Cambiar Contraseña" HorizontalAlignment="Left" Margin="78,160,0,0" VerticalAlignment="Top" Width="140" Click="CambiarContra_Click"/>
    </Grid>
</Window>
