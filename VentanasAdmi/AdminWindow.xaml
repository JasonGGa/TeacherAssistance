<Window x:Class="EpieHorarios.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EpieHorarios"
        mc:Ignorable="d"
        Title="Administración" Height="150" Width="300"
        Closed="Window_Closed">
    <Grid>
        <Button x:Name="AdminProfe" Content="Profesores" HorizontalAlignment="Left" Margin="50,40,0,0" VerticalAlignment="Top" Width="75" Click="AdminProfe_Click"/>
        <Button x:Name="AdminCurso" Content="Cursos" HorizontalAlignment="Left" Margin="170,40,0,0" VerticalAlignment="Top" Width="75" Click="AdminCurso_Click"/>
    </Grid>
</Window>
